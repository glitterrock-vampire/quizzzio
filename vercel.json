{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://quizzzio.onrender.com/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "dist/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
